/* DROP TABLE IF EXISTS tablea; */
CREATE TABLE IF NOT EXISTS tablea(
	start_date text 		/* eg. Jan 31/23 */ ,
	end_date text 		/* eg. Feb 1/23 */ ,
	subtotal real 		/* eg. 12.49 */ ,
	linenumber integer 		/* eg. 1 */ ,
	name text 		/* eg. brando */ ,
	timestamp text 		/* eg. 2023/02/23 12:30:00 */ 
);

